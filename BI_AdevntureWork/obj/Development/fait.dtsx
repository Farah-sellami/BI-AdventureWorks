                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    e.BusinessEntityID AS EmployeeID,           -- Lookup vers DimEmployee
    st.TerritoryID AS SalesTerritoryID,          -- Lookup vers DimSalesTerritory
    p.ProductID AS ProductID,                    -- Lookup vers DimProduct
    CONVERT(INT, FORMAT(o.OrderDate, 'yyyyMMdd')) AS OrderDateKey, -- 👉 Convertir directement ici
    o.TotalDue AS TotalSales,                    -- Mesure : Total des ventes
    e.VacationHours AS TotalVacationHours,       -- Mesure : Heures de vacances
