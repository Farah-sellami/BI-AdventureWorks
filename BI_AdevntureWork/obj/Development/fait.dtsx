    e.SalariedFlag AS TotalHoursWorked,          -- Remplacer Rate par SalariedFlag ou une autre mesure pertinente
    GETDATE() AS ModifiedDate                    -- Date du chargement (exécution de la requête)
FROM 
    dbo.EmployeeSA e
    INNER JOIN dbo.SalesOrderHeaderSA o ON e.BusinessEntityID = o.SalesPersonID
    INNER JOIN dbo.SalesOrderDetailSA sod ON o.SalesOrderID = sod.SalesOrderID
    INNER JOIN dbo.ProductSA p ON sod.ProductID = p.ProductID
    INNER JOIN dbo.SalesTerritorySA st ON o.TerritoryID = st.TerritoryID
    INNER JOIN dbo.DateSA dte ON CONVERT(INT, FORMAT(o.OrderDate, 'yyyyMMdd')) = dte.DateID;</property>
